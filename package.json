{
  "name": "wallet-management",
  "version": "0.0.0",
  "description": "CRUD Operation for wallet",
  "main": "build/index.js",
  "scripts": {
    "dev-start": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir build --source-maps inline",
    "watch-dev": "nodemon src/index.ts",
    "start": "node build/index.js",
    "build": "webpack",
    "build-models": "sequelize-auto -e mysql -o \"./src/models/sequalize\" -d wallet_system -h localhost -u root -p 3306 -x root -l ts"
  },
  "author": "Edward Jebaraj D",
  "devDependencies": {
    "@swc/cli": "0.3.10",
    "@swc/core": "1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.182",
    "@types/node": "^20.11.25",
    "concurrently": "^8.2.0",
    "eslint": "^8.57.0",
    "prettier": "^2.6.0",
    "prettier-eslint": "^13.0.0",
    "sequelize-cli": "^6.6.2",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.9.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "chokidar": "^3.5.3"
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "fs": "^0.0.1-security",
    "http-status": "^1.5.0",
    "https": "^1.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql2": "^3.14.1",
    "sequelize": "^6.37.7",
    "uuidv4": "^6.2.13"
  }
}
